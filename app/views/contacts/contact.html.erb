<div class="contact">
       <h1>Contact Us</h1>
      <%= form_for :contact,class: "form-horizontal"  do |f| %>
         <div class="form-group">
          <h2>First Name:</h2>
           <%= f.text_field :first_name, style:" width:97%; height:3em; display:block; border-radius:1em" %> <br>
         </div>
            <div class="form-group">
                <h2>Last Name:</h2>
               <%= f.text_field :last_name, style: " float:left; width:97%; height:3em; display:block; border-radius:1em" %> <br>
            </div> 
           
         
          <div class="form-group"> <br>
            <h2>Email:</h2>
            <%= f.email_field :email, style: " width:97%; height:3em; display:block; border-radius:1em" %>
          </div> 
          
         <div class="form-group"> <br>
             <h2 id="case">Case Description:</h2> 
            <%= f.text_area :case_description,style:"width:97%; height:8em; display:block;border-radius:1em"  %>
         </div>
           <div id="Submit">
             <%= f.submit "Submit", style:"margin-left:3em;margin-top:3em;" %>
           </div>
           
        
         
      <% end %>  
   

</div>
    
  </div>